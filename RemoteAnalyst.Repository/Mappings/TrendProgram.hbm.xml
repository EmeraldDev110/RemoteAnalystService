<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="RemoteAnalyst.Repository"
                   namespace="RemoteAnalyst.Repository.Models">

	<class name="TrendProgramInterval" table="TrendProgramInterval">
		<composite-id>
			<key-property name ="ProgramProfileId" column="ProgramProfileId" type="int"/>
			<key-property name="Interval" column="`Interval`" type ="datetime"/>
			<key-property name ="CpuNum" column="CpuNum" type="int"/>
			<key-property name="Pin" column="Pin" type ="int"/>
			<key-property name ="ProcessName" column="ProcessName" type="string" length="8"/>
			<key-property name ="Volume" column="Volume" type="string" length="8"/>
			<key-property name ="SubVolume" column="SubVolume" type="string" length="8"/>
			<key-property name ="FileName" column="FileName" type="string" length="8"/>
		</composite-id>
		<property name="CPUBusy" type="double" />
		<property name="QueueLength" type="double" />
		<property name="MsgRecdRate" type="double" />
		<property name="MsgSentRate" type="double" />
	</class>
	
	<class name="TrendProgramHourly" table="trendprogramhourly">
		<composite-id>
			<key-property name ="ProgramProfileId" column="ProgramProfileId" type="int"/>
			<key-property name="Hour" column="Hour" type ="datetime"/>
			<key-property name ="CpuNum" column="CpuNum" type="int"/>
			<key-property name="Pin" column="Pin" type ="int"/>
			<key-property name ="ProcessName" column="ProcessName" type="string" length="8"/>
			<key-property name ="Volume" column="Volume" type="string" length="8"/>
			<key-property name ="SubVolume" column="SubVolume" type="string" length="8"/>
			<key-property name ="FileName" column="FileName" type="string" length="8"/>
		</composite-id>
		<property name="PeakCPUBusy" type="double" />
		<property name="AverageCPUBusy" type="double" />
		<property name="PeakQueueLength" type="double" />
		<property name="AverageQueueLength" type="double" />
		<property name="PeakMsgRecdRate" type="double" />
		<property name="AverageMsgRecdRate" type="double" />
		<property name="PeakMsgSentRate" type="double" />
		<property name="AverageMsgSentRate" type="double" />
	</class>
</hibernate-mapping>