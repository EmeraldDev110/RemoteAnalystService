<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="RemoteAnalyst.Repository"
                   namespace="RemoteAnalyst.Repository.Models">

	<class name="RAInfo" table="rainfo">
		<id name="ID" column="ID">
			<generator class="native" />
		</id>
		<property name="QueryKey" column="queryKey" type="string" length="50" />
		<property name="QueryValue" column="queryValue" type="string" length="50" />
		<property name="Remark" type="string" length="200" />
	</class>
</hibernate-mapping>