<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="RemoteAnalyst.Repository"
                   namespace="RemoteAnalyst.Repository.Models">

	<class name="SystemInterval" table="systeminterval">
		<id name="ID">
			<generator class="native" />
		</id>
		<property name="SystemSerialNum" type="string" length="16" not-null="true" />
		<property name="DataDate" type="datetime" not-null="true" />
		<property name="Interval" column="`Interval`" type="int" not-null="true" />
	</class>
</hibernate-mapping>